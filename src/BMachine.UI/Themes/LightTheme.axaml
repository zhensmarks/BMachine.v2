<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Colors -->
    <Color x:Key="AppBackground">#F5F5F5</Color>
    <Color x:Key="CardBackground">#FFFFFF</Color>
    <Color x:Key="AccentBlue">#3b82f6</Color>
    <Color x:Key="AccentOrange">#f97316</Color>
    <Color x:Key="AccentPurple">#1d4ed8</Color>
    <!-- In Light Mode, Primary Text is Dark, Secondary is Gray -->
    <Color x:Key="TextPrimary">#121212</Color>
    <Color x:Key="TextSecondary">#666666</Color>
    
    <!-- Brushes -->
    <SolidColorBrush x:Key="AppBackgroundBrush" Color="{StaticResource AppBackground}"/>
    <SolidColorBrush x:Key="CardBackgroundBrush" Color="{StaticResource CardBackground}"/>
    <SolidColorBrush x:Key="AccentBlueBrush" Color="{StaticResource AccentBlue}"/>
    <SolidColorBrush x:Key="AccentOrangeBrush" Color="{StaticResource AccentOrange}"/>
    <SolidColorBrush x:Key="TextPrimaryBrush" Color="{StaticResource TextPrimary}"/>
    <SolidColorBrush x:Key="TextSecondaryBrush" Color="{StaticResource TextSecondary}"/>
    <SolidColorBrush x:Key="SeparatorBrush" Color="#E5E7EB"/>

    <!-- Log Brushes (Darker for Light Theme) -->
    <SolidColorBrush x:Key="LogInfoBrush" Color="#004d99"/>     <!-- Dark Blue -->
    <SolidColorBrush x:Key="LogSystemBrush" Color="#006666"/>   <!-- Dark Teal -->
    <SolidColorBrush x:Key="LogSuccessBrush" Color="#15803d"/>  <!-- Dark Green -->
    <SolidColorBrush x:Key="LogWarningBrush" Color="#c2410c"/>  <!-- Dark Orange -->
    <SolidColorBrush x:Key="LogErrorBrush" Color="#b91c1c"/>    <!-- Dark Red -->
    <SolidColorBrush x:Key="LogDebugBrush" Color="#525252"/>    <!-- Dark Gray -->

    <!-- TERMINAL SPECIFIC COLORS -->
    <SolidColorBrush x:Key="TerminalBackgroundBrush" Color="#F8F9FA"/> <!-- Light Gray BG for Light Theme -->
    <SolidColorBrush x:Key="TerminalTextBrush" Color="#1F2937"/>       <!-- Gray-800 -->
    <SolidColorBrush x:Key="TerminalInfoBrush" Color="#1d4ed8"/>       <!-- Blue-700 (Readable on White) -->
    <SolidColorBrush x:Key="TerminalSuccessBrush" Color="#15803d"/>    <!-- Green-700 -->
    <SolidColorBrush x:Key="TerminalWarningBrush" Color="#c2410c"/>    <!-- Orange-700 -->
    <SolidColorBrush x:Key="TerminalErrorBrush" Color="#b91c1c"/>      <!-- Red-700 -->
    <SolidColorBrush x:Key="TerminalSystemBrush" Color="#0f766e"/>     <!-- Teal-700 -->
    <SolidColorBrush x:Key="TerminalDebugBrush" Color="#4b5563"/>      <!-- Gray-600 -->

    <!-- Legacy/Missing Brushes -->
    <SolidColorBrush x:Key="BackgroundDarkBrush" Color="{StaticResource CardBackground}"/>
    <SolidColorBrush x:Key="AccentEditingBrush" Color="{StaticResource AccentBlue}"/>
    <SolidColorBrush x:Key="AccentRevisionBrush" Color="{StaticResource AccentOrange}"/>
    <SolidColorBrush x:Key="AccentLateBrush" Color="#ef4444"/>
    <SolidColorBrush x:Key="AccentRevisionLowOpacityBrush" Color="#33f97316"/>

    <!-- Dialog Accents -->
    <SolidColorBrush x:Key="AccentRedBrush" Color="#dc2626"/>
    <SolidColorBrush x:Key="AccentGreenBrush" Color="#16a34a"/>

     <!-- Default Shared Accents (Prevent crash/empty if ThemeService delays) -->
    <SolidColorBrush x:Key="AccentColorBrush" Color="{StaticResource AccentBlue}"/>
    <Color x:Key="AccentColor">#3b82f6</Color>
    
    <!-- Geometries are shared, so we can re-include them or just keep them in a shared dictionary. 
         For simplicity, I'll duplicate the geometries or ideally, move them to a Shared.axaml. 
         But since DarkTheme has them, and I'm swapping, I must include them here too to avoid missing resources if I swap completely.
         Wait, better approach: Extract Geometries to Icons.axaml and merge that in App.axaml globally.
         Then DarkTheme and LightTheme only contain Colors/Brushes.
         
         For now, to fix this quickly, I will include the geometries here too.
    -->
    
